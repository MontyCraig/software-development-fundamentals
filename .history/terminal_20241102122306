# First, navigate to your project directory
cd "/Volumes/One Touch/Meta Reps Core 2024/Meta Reps AI Software Company Code/Software Development Fundamentals"

# Initialize git repository
git init

# Create and configure .gitignore
echo "# Adding comprehensive .gitignore"
git add .gitignore

# Add all project files
echo "# Adding all project files"
git add .

# Create initial commit
git commit -m "Initial commit: Comprehensive software development fundamentals guide
- Core programming concepts
- Data structures and algorithms
- Development processes
- Software design principles
- Testing and QA
- Tools and technologies
- Documentation and examples"

# Create the repository using GitHub CLI
echo "# Creating GitHub repository"
gh repo create software-development-fundamentals --public --description "A comprehensive guide to software development fundamentals for students"

# Add the remote and push
git remote add origin https://github.com/YOUR_USERNAME/software-development-fundamentals.git
git branch -M main
git push -u origin main