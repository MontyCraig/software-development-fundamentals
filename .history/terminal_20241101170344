# Navigate to the main directory
cd "/Volumes/One Touch/Meta Reps Core 2024/Meta Reps AI Software Company Code/Software Development Fundamentals"

# Create temporary directory for merging
mkdir -p temp_merge

# Move all content from duplicate locations to temp directory
mv data_structures/* temp_merge/ 2>/dev/null
mv algorithms/* temp_merge/ 2>/dev/null
mv core_programming/* temp_merge/ 2>/dev/null
mv software_devlopment_fundamentals/* temp_merge/ 2>/dev/null

# Remove empty duplicate directories
rm -rf data_structures
rm -rf algorithms
rm -rf core_programming
rm -rf software_development_fundamentals

# Create the correct main directory if it doesn't exist
mkdir -p software_development_fundamentals

# Move all content to the correct location
mv temp_merge/* software_development_fundamentals/

# Clean up
rm -rf temp_merge

# Create the correct structure inside the main directory
cd software_development_fundamentals

# Create the proper folder structure
mkdir -p core_programming/data_structures_algorithms/{arrays,linked_lists,stacks,queues,trees,graphs,hash_tables}
